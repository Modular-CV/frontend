{
  "name": "frontend",
  "private": true,
  "type": "module",
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 4",
    "node": ">= 23"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production react-router build",
    "dev": "react-router dev --mode dev",
    "start": "cross-env NODE_ENV=production react-router-serve ./build/server/index.js",
    "typecheck": "react-router typegen && tsc --skipLibCheck",
    "lint": "echo Running Eslint ðŸ˜‰ && yarn eslint",
    "format": "echo Running Prettier ðŸ˜® && yarn prettier --write . --log-level error",
    "docker:build": "docker build -t app .",
    "vitest": "vitest --config ./tests/vitest/config.ts",
    "cypress:open": "yarn tailwind:watch & yarn cypress open --config-file ./tests/cypress/config.ts",
    "tailwind:watch": "yarn tailwindcss -i ./app/app.css -o ./tests/cypress/support/tailwind.css --watch"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.10.0",
    "@react-router/fs-routes": "^7.1.5",
    "@react-router/node": "^7.1.1",
